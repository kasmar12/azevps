        <argument android:name="iconType" app:argType="az.azerconnect.data.enums.PermissionIconType" />
        <argument android:name="permissions" app:argType="string[]" />
    </dialog>
    <fragment android:label="@string/in_select_contact_manual_nav_title" android:name="az.azerconnect.bakcell.ui.global.selectContact.manual.SelectContactManualFragment" android:id="@id/selectContactManualFragment" />
