        <action android:id="@id/action_selectContactFragment_to_selectContactManualFragment" app:destination="@id/selectContactManualFragment" />
        <action android:id="@id/action_selectContactFragment_to_permissionDialog" app:destination="@id/permissionDialog" />
    </fragment>
    <dialog android:label="@string/empty_label" android:name="az.azerconnect.bakcell.ui.global.permission.PermissionDialogFragment" android:id="@id/permissionDialog">
        <argument android:name="title" />
