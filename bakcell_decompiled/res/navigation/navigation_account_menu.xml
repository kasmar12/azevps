    <fragment android:label="@string/empty_label" android:name="az.azerconnect.bakcell.ui.main.more.accountMenu.primaryNumber.otp.LoginSecondaryAccountOtpFragment" android:id="@id/loginSecondaryAccountOtpFragment">
        <argument android:name="accountId" app:argType="integer" />
        <argument android:name="uuid" />
    </fragment>
    <action android:id="@id/action_global_balance_detail" app:destination="@id/balanceDetailNavId">
        <argument android:name="title" app:argType="string" />
