        <action android:id="@id/action_accountMenuDialogFragment_to_loggedInDevicesFragment" app:destination="@id/loggedInDevicesFragment" />
        <action android:id="@id/action_accountMenuDialogFragment_to_setPrimaryNumberWarningFragment" app:destination="@id/setPrimaryNumberWarningFragment" />
        <action android:id="@id/action_accountMenuDialogFragment_to_setPrimaryNumberOtpFragment" app:destination="@id/setPrimaryNumberOtpFragment" />
        <action android:id="@id/action_accountMenuDialogFragment_to_loginSecondaryAccountOtpFragment" app:destination="@id/loginSecondaryAccountOtpFragment" />
    </dialog>
    <dialog android:label="@string/empty_label" android:name="az.azerconnect.bakcell.ui.main.dashboard.balance.logOut.AccountExitDialogFragment" android:id="@id/accountExitDialogFragment">
        <argument android:name="accountId" app:argType="integer" />
    </dialog>
    <fragment android:label="@string/in_account_menu_dialog_logged_in_devices" android:name="az.azerconnect.bakcell.ui.main.more.settings.devices.LoggedInDevicesFragment" android:id="@id/loggedInDevicesFragment">
