<?xml version="1.0" encoding="utf-8"?>
<navigation android:id="@id/accountMenuNavId" app:startDestination="@id/accountMenuDialogFragment"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <include app:graph="@navigation/navigation_balance_detail" />
    <dialog android:label="@string/empty_label" android:name="az.azerconnect.bakcell.ui.main.more.accountMenu.AccountMenuDialogFragment" android:id="@id/accountMenuDialogFragment">
