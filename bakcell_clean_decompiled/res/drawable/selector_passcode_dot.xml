<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="@color/transparent" />
            <stroke android:width="1.0dip" android:color="?colorPrimary" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="oval">
            <solid android:color="@color/bkc_color_background_inverse" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke android:width="1.0dip" android:color="?passcodeDotNormalColor" />
        </shape>
    </item>
</selector>