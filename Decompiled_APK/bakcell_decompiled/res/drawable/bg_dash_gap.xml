<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="#10000000"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="?colorSurface" />
                    <corners android:radius="24.0dip" />
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <corners android:radius="24.0dip" />
                    <stroke android:width="1.0dip" android:color="?colorPrimary" android:dashWidth="2.0dip" android:dashGap="2.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:id="@android:id/mask">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/white" />
                    <corners android:radius="24.0dip" />
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <corners android:radius="24.0dip" />
                    <stroke android:width="1.0dip" android:color="?colorPrimary" android:dashWidth="2.0dip" android:dashGap="2.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
</ripple>