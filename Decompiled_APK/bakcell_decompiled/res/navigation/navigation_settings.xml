        <action android:id="@id/action_conditionRequirementsFragment_to_conditionRequirementsDetailFragment" app:destination="@id/conditionRequirementsDetailFragment" />
    </fragment>
    <fragment android:label="@string/in_settings_title_terms_conditions" android:name="az.azerconnect.bakcell.ui.main.more.settings.conditionRequirements.detail.ConditionRequirementsDetailFragment" android:id="@id/conditionRequirementsDetailFragment">
        <argument android:name="dto" app:argType="az.azerconnect.data.models.dto.ConditionRequirementsDto" />
    </fragment>
</navigation>